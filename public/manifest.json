{
  "manifest_version": 3,
  "name": "List-Extraction-Tool Extension",
  "version": "1.0",
  "description": "List Extraction Tool Extension",
  "background": {
    "service_worker": "./background.js"
  },
  "permissions": ["scripting", "activeTab"],
  "optional_host_permissions": ["*://*/*"],

  "short_name": "React Extension",
  "action": {
    "default_popup": "index.html",
    "default_title": "React Chrome Extension",
    "default_icon": {
      "16": "/images/react-logo-16.png",
      "32": "/images/react-logo-32.png",
      "48": "/images/react-logo-48.png",
      "128": "/images/react-logo-128.png"
    }
  },
  "icons": {
    "16": "/images/react-logo-16.png",
    "32": "/images/react-logo-32.png",
    "48": "/images/react-logo-48.png",
    "128": "/images/react-logo-128.png"
  }
}
