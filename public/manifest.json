{
  "manifest_version": 3,
  "name": "List-Extraction-Tool Extension",
  "version": "1.0",
  "description": "List Extraction Tool Extension",
  "background": {
    "service_worker": "./background.js"
  },
  "permissions": ["scripting", "activeTab"],
  "optional_host_permissions": ["*://*/*"],
  "content_scripts": [
    {
      "js": ["js/content.js"],
      "matches": ["*://*/*"]
    }
  ],
  "short_name": "List Extraction Tool Extension",
  "action": {
    "default_icon": {
      "16": "/images/logo-16.png",
      "32": "/images/logo-32.png",
      "48": "/images/logo-48.png",
      "128": "/images/logo-128.png"
    }
  },
  "icons": {
    "16": "/images/logo-16.png",
    "32": "/images/logo-32.png",
    "48": "/images/logo-48.png",
    "128": "/images/logo-128.png"
  }
}
